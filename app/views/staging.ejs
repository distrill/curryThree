<% include /metaPartials/top2 %>
<div class="content-container">

    <div class="not-carousel">
        <a href="#"><div class="elem nce-1">
            <div class="elem-mask"></div>
        </div></a>
        <a href="#"><div class="elem nce-2">
            <div class="elem-mask"></div>
        </div></a>
        <a href="#"><div class="elem nce-3">
            <div class="elem-mask"></div>
        </div></a>
        <a href="#"><div class="elem nce-4">
            <div class="elem-mask"></div>
        </div></a>
        <a href="#"><div class="elem nce-5">
            <div class="elem-mask"></div>
        </div></a>
    </div>

    <div class="staging-main-content-container">
    <%	for( var i in posts ) { %>
        <div class="news-elem">

            <!-- <img src="img/postThumbnails/<%= posts[ i ].thumbnail %>"> -->

            <a href="<%= posts[ i ].link %>">
                <img src="img/postThumbnails/<%= posts[ i ].thumbnail %>" class="news-elem-img">
            </a>
            <div class="news-elem-right">
                <span class="date-right">
                    <%= posts[ i ].date.toString().split( ' ' ).slice( 1, 3 ).join( ' ' ) + ', ' +
                        posts[ i ].date.toString().split( ' ' ).slice( 3, 4 ) %>
                </span>
                <p><a href="<%= posts[ i ].link %>"><%= posts[ i ].title %></a>
                </p>
                <div class="copy"><%= posts[ i ].body %></div>
                <% if( admin ) { %>
                    <div class="post-remove-edit">
                        <a href="post/<%=posts[ i ]._id %>">edit</a><br>
                        <a href="post/remove/<%= posts[ i ]._id %>">remove</a>
                    </div>
                <% } %>
            </div>

        </div>
    <% 	} %>
    </div>

    <div class="ad-fuck">
        <img src="img/temp-add-img.png" class="ad-image">
        <img src="img/temp-add-img.png" class="ad-image">
        <img src="img/temp-add-img.png" class="ad-image">
        <img src="img/temp-add-img.png" class="ad-image">
    </div>

    <!-- <div class="ad-column">
        what the heck -->
    <!-- </div> -->



</div>
